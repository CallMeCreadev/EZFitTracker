<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.healthapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_entry"
        android:name="com.example.healthapp.ui.entry.EntryFragment"
        android:label="@string/title_entry"
        tools:layout="@layout/fragment_entry" />

    <fragment
        android:id="@+id/navigation_database"
        android:name="com.example.healthapp.ui.database.DatabaseFragment"
        android:label="@string/title_database"
        tools:layout="@layout/fragment_database" />

    <fragment
        android:id="@+id/navigation_graph"
        android:name="com.example.healthapp.ui.graphs.GraphsFragment"
        android:label="@string/title_graph"
        tools:layout="@layout/fragment_graph" />
</navigation>
